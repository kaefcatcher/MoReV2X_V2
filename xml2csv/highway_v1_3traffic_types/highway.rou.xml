<?xml version="1.0" encoding="UTF-8"?>

<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">

    <!-- Defining vehicle types with different speeds -->
    <vType id="type_slow" accel="2.6" decel="4.5" sigma="0.5" length="5.0" minGap="2.5" maxSpeed="12.5" color="1,0,0"/>
    <vType id="type_medium" accel="2.6" decel="4.5" sigma="0.5" length="5.0" minGap="2.5" maxSpeed="25.0" color="0,1,0"/>
    <vType id="type_fast" accel="2.6" decel="4.5" sigma="0.5" length="5.0" minGap="2.5" maxSpeed="50.0" color="0,0,1"/>

    <!-- Defining vehicle flows in the forward direction (E0) -->
    <flow id="group1_forward" type="type_slow" begin="0" end="301" vehsPerHour="600" from="E0" to="E0"/>
    <flow id="group2_forward" type="type_medium" begin="0" end="301" vehsPerHour="600" from="E0" to="E0"/>
    <flow id="group3_forward" type="type_fast" begin="0" end="301" vehsPerHour="600" from="E0" to="E0"/>

    <!-- Defining vehicle flows in the reverse direction (E1) -->
    <flow id="group1_reverse" type="type_slow" begin="0" end="301" vehsPerHour="600" from="E1" to="E1"/>
    <flow id="group2_reverse" type="type_medium" begin="0" end="301" vehsPerHour="600" from="E1" to="E1"/>
    <flow id="group3_reverse" type="type_fast" begin="0" end="301" vehsPerHour="600" from="E1" to="E1"/>

</routes>
